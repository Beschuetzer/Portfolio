{
  "name": "portfolio_site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "final-form": "^4.20.2",
    "graphql-request": "^3.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "three": "^0.126.1",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "concurrently -k \"react-scripts start\" \"sass sass/main.scss src/css/style.css --watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "sass sass/main.scss src/css/style.comp.css",
    "concat:css": "concat src/css/style.concat.css -o src/css/style.comp.css",
    "prefix:css": "postcss src/css/style.comp.css -o src/css/style.prefix.css --use autoprefixer -b 'last 10 versions'",
    "compress:css": "sass src/css/style.prefix.css src/css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "watch:sass": "sass sass/main.scss src/css/style.css --watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concat": "^1.0.3",
    "concurrently": "^6.0.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.7"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
