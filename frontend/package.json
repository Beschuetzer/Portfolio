{
  "name": "portfolio_site",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "@tanstack/react-query": "5.67.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/howler": "^2.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.34",
    "@types/three": "^0.128.0",
    "axios": "^0.21.1",
    "detect-browser": "5.3.0",
    "final-form": "^4.20.2",
    "graphql-request": "^3.4.0",
    "howler": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-thumbnail-carousel": "1.5.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^6.1.15",
    "three": "^0.126.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "concurrently": "7.6.0",
    "cross-env": "^7.0.3",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.69.5",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "cross-env PORT=3010 concurrently -k \"react-scripts start --openssl-legacy-provider\" \"sass sass/main.scss src/css/style.css --watch\"",
    "build": "react-scripts build --openssl-legacy-provider",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "sass sass/main.scss src/css/style.comp.css",
    "concat:css": "concat src/css/style.concat.css -o src/css/style.comp.css",
    "prefix:css": "postcss src/css/style.comp.css -o src/css/style.prefix.css --use autoprefixer -b 'last 10 versions'",
    "compress:css": "sass src/css/style.prefix.css src/css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "watch:sass": "sass sass/main.scss src/css/style.css --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "16.x"
  }
}
